{
  "name": "recitation-points-tracker",
  "version": "1.0.0",
  "description": "A Quasar Project",
  "productName": "Recitation Points Tracker",
  "author": "Felix <jonfelixrico@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "test:unit:ui": "vitest --ui",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run",
    "test:e2e": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress open --e2e\"",
    "cypress:e2e:run": "cross-env NODE_ENV=test start-test \"quasar dev\" http-get://localhost:9000 \"cypress run --e2e\"",
    "test:e2e:ci": "firebase emulators:exec --import ./firebase/emulator-state \"pnpm cypress:e2e:run\"",
    "test:component": "cross-env NODE_ENV=test cypress open --component",
    "test:component:ci": "cross-env NODE_ENV=test cypress run --component",
    "firebase:emulators:start": "firebase emulators:start --import ./firebase/emulator-state",
    "firebase:emulators:save": "firebase emulators:export ./firebase/emulator-state"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^1.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "firebase": "^9.21.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.0.0",
    "@quasar/quasar-app-extension-testing": "^2.1.0",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^5.1.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.1.0",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/lodash": "^4.14.194",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitest/ui": "^0.15.0",
    "@vue/test-utils": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "cypress": "^12.2.0",
    "dotenv-defaults": "^5.0.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "firebase-tools": "^12.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vitest": "^0.15.0"
  },
  "engines": {
    "node": "^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
